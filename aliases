###########
# Generic #
###########
alias ls='ls --color=auto'
alias ll='ls -alF'
alias brew_install="brew leaves -r"

##############
# Go Aliases #
##############
# Get Go Install Path for module
alias go_install_path="go list -f '{{.Target}}'"
alias go_lambda_build="GOOS=linux GOARCH=arm64 go build -tags lambda.norpc -o bootstrap"
